@using Practice.Foundation.Infrastructure.Types;
@model List<BaseItem>

@{
    ViewData["Title"] = "Home Page";
}

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>
                Successfull
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>
                    @obj.GetLanguageField("name")
                </td>
                <td>
                    @obj.GetLanguageField("Successfull")
                </td>
            </tr>
        }
    </tbody>
</table>
